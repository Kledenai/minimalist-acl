{
  "name": "minimalist-acl-test",
  "version": "0.0.3",
  "description": "Minimalistic ACL implementation for privileges management in javascript",
  "main": "lib/acl.js",
  "types": "lib/acl.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch \"src//\" --exec \"ts-node src/acl.ts\" -e ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config jestconfig.json",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kledenai/minimalist-acl"
  },
  "keywords": [
    "javascript",
    "acl",
    "roles",
    "permissions",
    "privileges",
    "minimalist",
    "authorization"
  ],
  "author": "Kledenai Ashver",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kledenai/minimalist-acl/issues"
  },
  "homepage": "https://github.com/Kledenai/minimalist-acl",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.1",
    "jest": "^27.2.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "lib/**/*"
  ]
}
